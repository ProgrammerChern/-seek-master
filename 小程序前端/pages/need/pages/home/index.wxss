.insrructions-container .content {
  padding: 0 30rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333;
}

.insrructions-container .content .title {
  display: inline-block;
  background: #fff7f1;
  text-align: center;
  width: 175rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #ff7043;
  font-size: 30rpx;
  font-weight: 600;
  border-top-right-radius: 25rpx;
  border-bottom-right-radius: 25rpx;
  margin-bottom: 20rpx;
}

.insrructions-container .content .cell {
  font-size: 28rpx;
  font-weight: 400;
  color: #333;
  margin-bottom: 20rpx;
}

.insrructions-container .content .useprocess {
  text-align: center;
  margin-bottom: 30rpx;
}

.insrructions-container .content .useprocess image {
  width: 669rpx;
  height: 130rpx;
}

.insrructions-container .content .promise {
  text-align: left;
  font-size: 32rpx;
  font-weight: 600;
  color: #333;
}

.insrructions-container .content .promise image {
  position: relative;
  top: 6rpx;
  width: 32rpx;
  height: 32rpx;
  margin-right: 12rpx;
}

.insrructions-container .content .promise .block view {
  margin-left: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #666;
  margin-bottom: 30rpx;
}

.insrructions-container .content .tips {
  margin-bottom: 40rpx;
}

.insrructions-container .content .tips image {
  width: 690rpx;
  height: 166rpx;
}

.insrructions-container .content .btn {
  width: 589rpx;
  height: 90rpx;
  margin: 0 auto;
  padding: 0;
  line-height: 90rpx;
  text-align: center;
  background: #00c87c;
  border-radius: 46rpx;
  color: #fff;
  font-size: 34rpx;
  font-weight: 400;
}

.insrructions-container .content .btn-hover {
  opacity: .6;
}
@-webkit-keyframes fadeIn {
  0% {
      opacity: 0;
  }

  to {
      opacity: 1;
  }
}

@keyframes fadeIn {
  0% {
      opacity: 0;
  }

  to {
      opacity: 1;
  }
}

.fadeIn {
  animation-name: fadeIn;
}

@keyframes fadeInUp {
  0% {
      opacity: 0;
      transform: translate3d(0,100%,0);
  }

  to {
      opacity: 1;
      transform: none;
  }
}

.fadeInUp {
  animation-name: fadeInUp;
}

@keyframes fadeInDown {
  0% {
      opacity: 0;
      transform: translate3d(0,-100%,0);
  }

  to {
      opacity: 1;
      transform: none;
  }
}

.fadeInDown {
  animation-name: fadeInDown;
}

.animated {
  animation-duration: 0.5s;
  animation-fill-mode: both;
}
.popup-container {
  width: 100%;
  height: 100%; position: relative;
}
.popup-container .popup-mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(22,22,22,0.6);
  z-index: 9999;
}

.popup-container .popup-mask .popup {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  z-index: 999999;
}

.radio-containor .button {
  display: flex;
}

.radio-containor .button .yes {
  width: 119rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid #dddddd;
  box-sizing: border-box;
  border-radius: 5rpx;
  background: #fff;
  font-size: 26rpx;
  color: #333;
  text-align: center;
  margin-right: 10rpx;
}

.radio-containor .button .no {
  width: 119rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 5rpx;
  box-sizing: border-box;
  background: #f9f9fa;
  font-size: 26rpx;
  text-align: center;
}


.agreement-container {
  padding: 45rpx 0;
  background: #fff;
  width: 750rpx;
  border-radius: 10rpx 10rpx 0px 0px;
}

.agreement-container .title {
  text-align: center;
  font-size: 34rpx;
  font-weight: 600;
  color: #333;
  margin-bottom: 60rpx;
}

.agreement-container .content {
  text-align: center;
}

.agreement-container .content .description {
  text-align: left;
  padding: 0 60rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #999;
  margin-bottom: 30rpx;
}

.agreement-bottom {
