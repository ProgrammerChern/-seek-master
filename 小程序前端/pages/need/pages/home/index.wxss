.number-container .number {
  display: flex;
  border: 2rpx solid #d8d8d8;
  border-radius: 4rpx;
  overflow: hidden;
}

.number-container .number .block {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 70rpx;
}

.number-container .number .subtract-block {
