  width: 88rpx;
  height: 44rpx; padding-right: 0;
}
.crumb-containor{
  position: relative;
}
.cu-form-group .title {
  min-width: 150rpx;
}
.crumb-containor .value {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-right: 30rpx;
}
.crumb-containor .value::after {
  font-family: "cuIcon";
  display: block;
  content: "\e6a3";
  position: absolute;
  font-size: 34rpx;
  color: var(--grey);
  line-height: 100rpx;
  width: 60rpx;
  text-align: center;
  top: 0;
  bottom: 0;
  right: 10rpx;
  margin: auto;
}

.scope-containor {
  background: #fff;
  border-radius: 2rpx;
  width: 750rpx;
  overflow: hidden;
}

.scope-containor .content {
  
  border: 0;
}

.scope-containor .content .close {
  position: absolute;
  right: 0;
  top: 0;
  text-align: right;
}

.scope-containor .content .close image {
  width: 52rpx;
  height: 52rpx; margin:10rpx 20rpx;
}

.scope-containor .content .title {
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #333333;
  text-align: center;
}

.scope-containor .content .ok {
