.repair .cu-list.menu-avatar>.cu-item {
    padding-left: 50rpx;
}
.ob-options-view {
    width: 750rpx;
    height: 100rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    position: relative;
    flex-shrink: 0;
    flex-grow: 0;
    background: white;
}

.ob-options-list {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
}

.ob-options-item {
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32rpx;
    color: #99acb1;
    width: 100%;
    height: 100%;
    position: relative;
}

.ob-item-sep-line {
    width: 1rpx;
    height: 100%;
    background-color: #f0f0f0;
}

.ob-reddot {
    width: 10rpx;
    height: 10rpx;
    background: red;
    border-radius: 50%;
    float: right;
    position: absolute;
    right: 50rpx;
    top: 25rpx;
}

.ob-options-bottomLine {
    width: 100%;
    height: 2rpx;
    position: absolute;
    bottom: 0rpx;
    left: 0rpx;
    background: #f0f0f0;
    z-index: 90;
}

.ob-options-indicator {
    width: 84rpx;
    height: 4rpx;
    background-color: #333;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    z-index: 99;
}

.release {
    text-align: center;
}

.release .icon image {
    width: 396rpx;
    height: 396rpx;
}

.release .desciption {
    padding: 45rpx;
    font-size: 32rpx;
    color: #333;
}

.release .useprocess image {
    width: 669rpx;
    height: 130rpx;
}

.qp-container .body-container {
}

.qp-container .body-container .content {
    margin-bottom: 30rpx;
    background: #fff;
}

.qp-container .body-container .arrow {
    position: relative;
    left: 30rpx;
    margin-top: -20rpx;
    width: 0rpx;
    height: 0rpx;
    border: 15rpx solid transparent;
    border-bottom: 15rpx solid #f9f9fa;
}

.qp-container .body-container .des {
    padding: 25rpx;
    font-size: 26rpx;
    font-weight: 400;
    color: #333333;
    background: #f9f9fa;
}

.qp-container .body-container .des view text:nth-child(1) {
    font-size: 26rpx;
    color: #6a6a6a;
}

.qp-container .body-container .hirebtn {
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    height: 88rpx;
    background: #fff;
}

.qp-container .body-container .hirebtn view {
    width: 330rpx;
}

.qp-container .body-container .hirebtn image {
    position: relative;
    top: 3rpx;
    width: 24rpx;
    height: 24rpx;
    margin-right: 12rpx;
}

.qp-container .body-container .hirebtn text {
    color: #333;
    font-size: 30rpx;
    font-weight: 400;
}

.qp-container .body-container .cell {
    padding-top: 20rpx;
    width: 690rpx;
    color: #999;
}

.qp-container .body-container .cell view {
    width: 315rpx;
    float: left;
}

.qp-container .body-container .cell view:nth-child(1) {
    font-weight: 600;
    padding-left: 30rpx;
    font-size: 30rpx;
    color: #333;
}

.qp-container .body-container .cell view:nth-child(2) {
    text-align: right;
    font-size: 26rpx;
    font-weight: 400;
}

.qp-container .body-container .promise {
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 24rpx;
    color: #666;
    background: #fff;
    height: 80rpx;
}

.qp-container .body-container .promise .block {
    display: flex;
    align-items: center;
    height: 60rpx;
}

.qp-container .body-container .promise .block image {
    margin-right: 12rpx;
    width: 30rpx;
    height: 30rpx;
}
.card-container {
    display: flex;
    align-items: center;
    width: 690rpx;
    padding: 30rpx;
    background: #fff;
}

.card-container .avater {
    height: 99rpx;
    width: 96rpx;
    margin-right: 30rpx;
}

.card-container .avater image {
    height: 99rpx;
    width: 96rpx;
    border-radius: 50%;
    border: 1rpx solid #eaeaea;
}

.card-container .card-content {
    display: flex;
    flex-direction: column;
    height: 99rpx;
    justify-content: space-around;
}

.card-container .card-cell {
    display: flex;
    align-items: center;
    font-size: 30rpx;
    color: #333;
}

.card-container .card-cell .realname {
    font-weight: 600;
    line-height: 100%;
}

.card-container .card-cell .distance {
    display: flex;
    font-size: 25rpx;
    color: #999;
}

.card-container .card-cell .distance image {
    position: relative;
    top: 2rpx;
    width: 20rpx;
    height: 24rpx;
    margin-right: 8rpx;
}

.card-container .card-cell .services {
    display: flex;
    align-items: center;
    font-size: 25rpx;
    color: #999;
    font-weight: 400;
}

.card-container .card-cell .arrowicon {
    display: flex;
}

.card-container .card-cell .arrowicon image {
    width: 32rpx;
    height: 32rpx;
}

.card-container .card-cell .icon {
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20rpx;
    font-weight: 400;
    margin-left: 20rpx;
}

.card-container .card-cell .icon view {
    display: flex;
    align-items: center;
    position: relative;
    height: 26rpx;
    border: 2rpx solid #b79359;
    box-sizing: border-box;
    border-left: transparent;
    border-radius: 0 12.5rpx 12.5rpx 0;
    color: #b79359;
    margin-right: 30rpx;
}

.card-container .card-cell .icon view .protect {
    position: absolute;
    left: -20rpx;
    width: 33rpx;
    height: 33rpx;
}

.card-container .card-cell .icon view .aroow_r {
    width: 20rpx;
    height: 20rpx;
    margin-right: 2rpx;
}

.card-container .card-cell .icon view text {
    padding: 0 10rpx 0 15rpx;
}

.card-container .mobile {
    position: absolute;
    right: 30rpx;
}

.card-container .mobile image {
    width: 67rpx;
    height: 65rpx;
}

.card-container .link {
    position: absolute;
    right: 30rpx;
}

.card-container .link image {
    width: 16rpx;
    height: 25rpx;
}

.icon-titles {
    width: 6rpx;
    height: 26rpx;
    border-radius: 3rpx solod #00c87c;
    background-color: #00c87c;
    margin-right: 15rpx;
}