page {
    background: #fff!important;
}

swiper {
    height: 600rpx;
    width: 90%;
    border-bottom: 1px solid rgba(0,0,0,0.05);
    margin: auto;
    overflow: hidden;
    border-radius: 10rpx;
    margin-top: 40rpx;
}

swiper .slide-image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
}